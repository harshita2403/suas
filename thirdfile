import java.util.Scanner;
		 
public class third
{
    public static void main(String args[])
    {
       int a, b, res;
       Scanner scan = new Scanner(System.in);
	   
       System.out.print("Enter Two Numbers : ");
       a = scan.nextInt();
       b = scan.nextInt();
	   
       res = a + b;
       System.out.println("Addition = " +res);
	   
       res = a * b;
       System.out.println("Multiplication = " +res);
	   
    }
}
